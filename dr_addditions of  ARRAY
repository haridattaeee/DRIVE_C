#include <stdio.h>

int main()
{
    int i,marks[100],size,e=0,o=0;
    scanf("%d",&size);
    
    for(i=0;i<size;i++)
    {
        scanf("%d",&marks[i]);
    }
     for(i=0;i<size;i++)
    {
        if(marks[i]%2==0)
        {
            e++;
        }
        else
        {
            o++;
        }
        
    }
   printf("even %d  odd %d ",e,o);

    return 0;
}
