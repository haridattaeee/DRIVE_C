
#include <stdio.h>

int big_num(int a,int b,int c,int d)
{
    if ((a>b) && (a>c) && (a>d))
    {
        return a;
    }
    else if ((b>c) && (b>d))
    {
        return b;
    }
    else if (c>d)
    {
        return c;
    }
    else
    {
        return d;
    }
}

int main()
{
    int a , b,c,d,ans;
    scanf("%d%d%d%d",&a,&b,&c,&d);
    
    ans=big_num(a,b,c,d);
    printf(" the big number is %d",ans);
    
   // printf("Hello World");

    return 0;
}
