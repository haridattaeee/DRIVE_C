#include <stdio.h>
#include <stdlib.h>

int main()
{
 //A>B, A<B,A=B,
   int a=10,b=20;
    if(a>b)//--10>20
    {
     printf("A > B");  //a=10+1;
    }
    else if(a==b)//10==20
    {
        printf("A==B");
    }
else{
    printf("");
}
    
    //printf("%d",a);
    //printf("Hello world!\n");
    return 0;
}




