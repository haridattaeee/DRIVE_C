#include <stdio.h>
void pattern(int num)
{
    int i,j;
    for(i=1;i<=num;i++)
    {
        for(j=0;j<num;j++)
        {
            printf("%d",i);
     
        }
        printf("\n");
    }
    
}



int main()
{
    int i;
    scanf("%d",&i);
    pattern(i);
    
 
    return 0;
}



