#include<stdio.h>
int main()
{
    int a,b,low,lcm;                           // 12 8
    scanf("%d%d",&a,&b);

    low=(a<b)? a:b;
    while(1)
    {
        if (low%a==0 && low%b==0)
        {
            lcm=low;
            break;
        }
        low ++;
    }
    printf("lcm of two numbers is %d",lcm);        //24
}

