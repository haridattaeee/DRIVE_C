
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,gcd,i;                              // 12 24 
    printf("ENTER THE TWO NUMBERS \n");
    scanf("\n%d%d",&a,&b);
    for(i=1;i<=a && i<=b;i++)     
    {
        if (a%i==0 && b%i==0 )
        {
            gcd=i;                         // 12
        }

    }
    printf("\n GDC OF TWO NUMBERS IS %d \n",gcd);
    return 0;
}
