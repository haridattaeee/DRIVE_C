void gen_fib_compvalue(int num){   //5
int a=0,b=1,c;

       //5
if(num==0){
            printf("True");
            return ;

}
if(num == 1)
{
    printf("True");
return;
}
while(c<=num){

c=a+b;  //
a=b;
b=c;



if(c== num){
    printf("True");
return;
}


    }

printf("Flase");
    return;

}



int main()
